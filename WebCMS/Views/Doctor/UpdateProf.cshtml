@{




    <form asp-action="UpdateProf" method="post">


        <div>
            <label for="FullName" class="form-label">Full Name</label>
            <input type="text" name="fullName" id="fullName" class="form-control" value="@Model.FullName" required />
        </div>

        <div>
            <label for="Specialization" class="col-form-label">Specialization</label>
            <input type="text" name="Specialization" id="Specialization" class="form-control" value="@Model.Specialization" required />

        </div>

        


        <button type="submit" class="btn btn-primary m-5">Edit Profile</button>
        
    </form>


}
